def nine():
    colour = np.random.rand(1, 3)
    t.color(colour)
    for i in range(3):
        square()
        t.fd(15)
    t.right(90)
    t.fd(15)
    t.left(90)
    square()
    t.penup()
    t.backward(60)
    t.pendown()
    square()
    t.right(90)
    t.fd(15)
    t.left(90)
    t.fd(15)
    for i in range(4):
        square()
        t.fd(15)
    t.right(90)
    t.fd(15)
    t.left(90)
    t.backward(15)
    square()
    t.right(90)
    t.fd(15)
    t.left(90)
    t.backward(45)
    for i in range(3):
        square()
        t.fd(15)
    t.penup()

cyfry = { '0': zero,
          '1' : one,
          '2' : two,
          '3' : three,
          '4' : four,
          '5' : five,
          '6' : six,
          '7' : seven,
          '8' : eight,
          '9' : nine}

a = input()

count = 0
y = 0
for e in a:
    t.setposition(count,y)
    t.pendown()
    cyfry[e]()
    count += 90



print(a)

turtle.exitonclick()
