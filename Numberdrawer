import turtle
import numpy as np
import time
t = turtle.Turtle()
t.speed('fastest')

def square():
    t.begin_fill()
    for i in range(4):
        t.forward(15)
        t.right(90)
    t.end_fill()


def zero():
    colour = np.random.rand(1, 3)
    t.color(colour)
    square()
    for i in range(2):
        t.fd(15)
        square()
    t.fd(15)
    t.right(90)
    t.fd(15)
    t.left(90)
    for x in range(3):
        square()
        t.right(90)
        t.fd(15)
        t.left(90)
    t.backward(45)
    for i in range(3):
        square()
        t.fd(15)
    t.penup()
    t.backward(45)
    t.left(90)
    t.fd(45)
    t.left(90)
    t.forward(15)
    t.right(180)
    t.pendown()
    for x in range(3):
        square()
        t.right(90)
        t.fd(15)
        t.left(90)
    t.penup()

def one():
    colour = np.random.rand(1, 3)
    t.color(colour)
    square()
    t.penup()
    t.right(90)
    t.fd(15)
    t.left(90)
    t.backward(15)
    t.pendown()
    square()
    t.fd(15)
    square()
    for i in range(3):
        t.right(90)
        t.fd(15)
        t.left(90)
        square()
    t.fd(15)
    square()
    t.backward(30)
    square()
    t.penup()

def two():
    colour = np.random.rand(1, 3)
    t.color(colour)
    for i in range(3):
        square()
        t.fd(15)
    t.right(90)
    t.fd(15)
    t.left(90)
    square()
    t.penup()
    t.backward(60)
    t.pendown()
    square()
    t.right(90)
    t.fd(15)
    t.left(90)
    t.fd(15)
    t.penup()
    t.forward(15)
    t.pendown()
    for i in range(2):
        square()
        t.fd(15)
    t.backward(30)
    t.right(90)
    t.fd(15)
    square()
    t.fd(15)
    t.left(90)
    t.fd(30)
    square()
    t.backward(60)
    for i in range(4):
        square()
        t.fd(15)
    t.penup()


