    t.right(90)
    t.fd(15)
    t.left(90)
    t.backward(15)
    t.pendown()
    square()
    t.right(90)
    t.fd(15)
    t.left(90)
    for i in range(4):
        square()
        t.fd(15)
    t.right(90)
